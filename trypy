a = [7, 23, -6]

try:
    n = int(input())
    7 / n
    print(a[n])
except (ValueError, IndexError):
    print("Неверное значение n!")
# except IndexError:
#     print("В списке только 3 элемента")
except ZeroDivisionError:
    print("деление на ноль!")
except:
    print("какая-то другая ошибка")
else:
    print("ошибок нет!")
finally:
    print("the end")